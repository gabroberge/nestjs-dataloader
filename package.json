{
  "name": "@gabroberge/nestjs-dataloader",
  "version": "1.2.1",
  "description": "A NestJS decorator for dataloader",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.spec.ts\"",
    "lint": "tsc",
    "ci": "pnpm lint && pnpm test && pnpm build",
    "release": "pnpm ci && changeset publish",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "pnpm format"
    ]
  },
  "keywords": [
    "nestjs",
    "dataloader",
    "graphql",
    "typescript"
  ],
  "author": "Gabriel Roberge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabroberge/nestjs-dataloader.git"
  },
  "homepage": "https://github.com/gabroberge/nestjs-dataloader#readme",
  "bugs": "https://github.com/gabroberge/nestjs-dataloader/issues",
  "peerDependencies": {
    "@nestjs/common": "^10.0.1",
    "@nestjs/core": "^10.0.1",
    "@nestjs/graphql": "^12.0.1",
    "dataloader": "^2.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/testing": "^10.2.10",
    "@types/node": "^20.10.0",
    "dataloader": "^2.2.2",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "rxjs": "^7.8.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "unplugin-swc": "^1.4.3",
    "vitest": "^0.34.6"
  }
}
